
@{
    Layout = null;
}

@model MyMvc.Models.ParkInfoViewModel



<link href="~/Content/bootstrap.css" rel="stylesheet" />
<link href="~/AdmianInter/layui/css/layui.css?" rel="stylesheet" />



@using (Html.BeginForm())
{
    <body class="childrenBody">
        <form class="layui-form" style="width:80%;">
            <div class="layui-form-item">
                <label class="layui-form-label">级别</label>
                <div class="layui-input-block">
                    <input type="text" class="layui-input userName" name="Tname" lay-verify="required" placeholder="请输入级别">
                </div>
            </div>
            <div class="layui-form-item">
                <label class="layui-form-label">费用</label>
                <div class="layui-input-block">
                    <input type="text" class="layui-input userName" name="Tmaney" lay-verify="required" placeholder="请输入费用">
                </div>
            </div>
            <div class="layui-form-item">
                <div class="layui-input-block">
                    <input type="submit" class="btn btn-success" value="立即提交" />
                    <button type="reset" class="btn btn-danger" onclick="location.href='/UserMvc/CarTypesIndex'">取消提交</button>
                    <button type="reset" class="btn btn-primary">重置数据</button>

                </div>
            </div>
        </form>


    </body>
}







